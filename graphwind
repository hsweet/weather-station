#set terminal pngcairo transparent truecolor size 800,640 enhanced font "Times,16" 
#set output 'windlog.png'
# set timestamp
set grid
set linetype 2 lw 4 lc rgb "blue" pointtype 6
set linetype 1 lw 2 lc rgb "green" pointtype 16
set title "Wind Speed\nThe Birch School"
set xdata time
set timefmt x "%Y-%m-%d-%H:%M:%S"
# month,day newline, hour and minute
#set format x "%m/%d\n%H:%M"
set format x "%H:%M\n%m/%d/"
set xlabel "Time"
set ylabel "Speed"
#plot "windlog.txt" using 1:3 title "Average Speed" with lines, "windlog.txt" using 1:2 title "Instananeous Speed" with lines
plot "windlog.txt" using 1:2 title "Miles Per Hour" with lines, "windlog.txt" using 1:3 title "Average Speed "with lines
 
